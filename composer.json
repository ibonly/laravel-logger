{
    "name": "ibonly/laravel-logger",
    "description": "Laravel activity logger",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Adeniyi Ibraheem",
            "email": "ibonly01@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/config": "~5.5.0|~5.6.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/support": "~5.5.0|~5.6.0"
    },
    "autoload": {
        "classmap": ["src/"],
        "psr-4" : {
            "Ibonly\\LaravelLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["test/"],
        "psr-4": {
            "Ibonly\\LaravelLogger\\Test\\": "test"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish"
        ]
    }
}
